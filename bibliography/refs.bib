file = {:home/oscar/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Barrag{\'{a}}n et al. - 2016 - K2-98b A 32 M ⊕ NEPTUNE-SIZE PLANET IN A 10 DAY ORBIT TRANSITING AN F8 STAR(2).pdf:pdf},
issn = {1538-3881},
journal = {AJ},
keywords = {detection,epic 211391664b,fundamental parameters,individual,planets and satellites,stars},
month = {nov},
number = {6},
pages = {193},
title = {{K2-98b: A 32 M ⊕ NEPTUNE-SIZE PLANET IN A 10 DAY ORBIT TRANSITING AN F8 STAR}},
url = {http://arxiv.org/abs/1608.01165 http://stacks.iop.org/1538-3881/152/i=6/a=193?key=crossref.9c74d9771cd2701ef81e7305ec423010},
