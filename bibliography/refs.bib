@article{Barragan2017,
abstract = {We announce the discovery of EPIC218916923 b, a transiting warm-Jupiter ($T_\mathrm{eq}$=555$\pm$11 K) on a 29-day orbit around an active (log$R^\prime_\mathrm{HK}$ = $-$4.49 $\pm$ 0.03) K0 V star in K2 Campaign 7. We derive the system's parameters by combining the K2 photometry with ground-based follow-up observations. With a mass of $M_\mathrm{p}=0.381 \pm 0.045 M_{\rm Jup}$ and radius of $R_\mathrm{p}= 0.812 \pm 0.030 R_{\rm Jup}$, EPIC218916923 b is one of the transiting warm Jupiters with the lowest mass known to date. The planetary mean density $\rho_\mathrm{p} = 0.88 \pm 0.14 {\rm g\,cm^{-3}}$ can be explained with a core of $\sim 50 M_\oplus$. Given the brightness of the host star ($V$= 11.653 mag), the relatively short transit duration ($\sim$5 hours), and the expected amplitude of the Rossiter-McLaughlin effect ($\sim$25 ms), EPIC218916923 is an ideal target to measure the spin-orbit angle of a planetary system hosting a warm Jupiter.},
annote = {NULL},
archivePrefix = {arXiv},
arxivId = {1702.00691},
author = {Barrag{\'{a}}n, Oscar and Gandolfi, D. and Smith, A. M. S. and Deeg, H. J. and Fridlund, M. C. V. and Persson, C. M. and Donati, P. and Endl, M. and Csizmadia, Sz. and Grziwa, S. and Nespral, D. and Hatzes, A. P. and Cochran, W. D. and Fossati, L. and Brems, S. S. and Cabrera, J. and Cusano, F. and Eigm{\"{u}}ller, Ph. and Eiroa, C. and Erikson, A. and Guenther, E. and Korth, J. and Mancini, L. and P{\"{a}}tzold, M. and Prieto-Arranz, J. and Rauer, H. and Rebollido, I. and Saario, J. and Zakhozhay, O. V.},
